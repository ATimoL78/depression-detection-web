VITE_OAUTH_PORTAL_URL=https://oauth.example.com
VITE_APP_ID=depression-detection
VITE_APP_TITLE=抑郁症检测系统
VITE_APP_LOGO=https://placehold.co/128x128/E1E7EF/1F2937?text=Depression
VITE_ANALYTICS_ENDPOINT=
VITE_ANALYTICS_WEBSITE_ID=
SESSION_SECRET=PLEASE_CHANGE_THIS_IN_PRODUCTION
NODE_ENV=production

# 豆包 AI API配置 (兼容 OpenAI 格式)
OPENAI_API_KEY=${DOUBAO_API_KEY}
OPENAI_BASE_URL=https://ark.cn-beijing.volces.com/api/v3
OPENAI_MODEL=ep-20241115031255-xnfmq

# 数据库配置 (PostgreSQL)
DATABASE_URL=${DATABASE_URL}

# 性能优化
COMPRESSION_ENABLED=true
CACHE_ENABLED=true
CACHE_MAX_AGE=86400

# 日志级别
LOG_LEVEL=info

# CORS配置
CORS_ORIGIN=*

# 限流配置
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
